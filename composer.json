{
    "name": "ecomailcz/gosms-client",
    "description": "Client for GoSms.cz api",
    "license": ["MIT"],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8"
    },
    "autoload": {
		"psr-4": {
			"EcomailGoSms\\": ["src/"]
		}
	},
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
